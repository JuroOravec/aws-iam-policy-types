{
  "name": "aws-iam-policy-types",
  "version": "1.0.2",
  "private": false,
  "description": "Autogenerated Typescript types for AWS IAM Policy and enums for all policy actions",
  "author": "Juro Oravec <juraj.oravec.josefson@gmail.com>",
  "homepage": "https://github.com/jurooravec/aws-iam-policy-types#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jurooravec/aws-iam-policy-types.git"
  },
  "bugs": {
    "url": "https://github.com/jurooravec/aws-iam-policy-types/issues"
  },
  "license": "MIT",
  "keywords": [
    "aws",
    "iam",
    "types",
    "typescript",
    "typing",
    "policy",
    "action"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/cjs/index.js"
  },
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.js",
  "bin": "dist/cjs/cli/index.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "npm run start:dev",
    "start:prod": "node dist/cjs/index.js",
    "start:dev": "ts-node ./src/index.ts",
    "build": "npm run build:cjs",
    "build:all": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "gen:docs": "typedoc src/index.ts --plugin typedoc-plugin-markdown --tsconfig tsconfig.base.json --out ./docs/typedoc",
    "extract": "ts-node ./src/extract.ts"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.6",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.4.1",
    "jsdom": "^24.0.0",
    "prettier": "^2.5.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.25.1",
    "typedoc-plugin-markdown": "^3.16.0",
    "typescript": "^5.0.4",
    "vitest": "^0.30.1"
  }
}
